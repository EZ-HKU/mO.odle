{
  "manifest_version": 3,
  "name": "mO.odle",
  "version": "1.0",
  "description": "Make Moodle great again!",
  "content_scripts": [
    {
      "js": [
        "./js/content.js"
      ],
      "css": [
        "./css/styles.css"
      ],
      "matches": [
        "https://moodle.hku.hk/"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }, 
  "permissions": ["storage"], 
  "icons": {
    "16": "./images/icon.png",
    "48": "./images/icon.png",
    "128": "./images/icon.png"
  }
}